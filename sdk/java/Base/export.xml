<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="${ant.project.name}">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="bin/export.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="examples.ArraySortBenchmark"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset excludes="*.jar" dir="bin"/>
            <zipfileset excludes="META-INF/*.SF" src="../Base/libs/log4j-1.2.17.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="../Base/libs/charts4j-1.3.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="../Base/libs/caliper-1.0-SNAPSHOT.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="../Base/libs/gson-2.2.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="../Base/libs/guava-13.0.1.jar"/>
        </jar>
    </target>
</project>
